{% extends 'base.html.twig' %}

{% block title %}FeesValue index{% endblock %}

{% block body %}
    <h1>FeesValue index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Amount</th>
                <th>CreatedAt</th>
                <th>Description</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for fees_value in fees_values %}
            <tr>
                <td>{{ fees_value.id }}</td>
                <td>{{ fees_value.amount }}</td>
                <td>{{ fees_value.createdAt ? fees_value.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ fees_value.description }}</td>
                <td>
                    <a href="{{ path('fees_value_show', {'id': fees_value.id}) }}">show</a>
                    <a href="{{ path('fees_value_edit', {'id': fees_value.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('fees_value_new') }}">Create new</a>
{% endblock %}
